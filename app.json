{
  "expo": {
    "name": "Pro Recrute GBG",
    "slug": "pro-recrute-gbg",
    "version": "1.0.0",
    "scheme": "prorecruteapp",
    "owner": "weexduunx88",
    "description": "Pro Recrute GBG is a recruitment app designed to connect job seekers with employers in the Greater Banjul Area, Gambia. It provides a platform for users to search for job opportunities, apply for positions, and manage their profiles.",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.weexduunx88.prorecrutegbg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "dark": {
            "image": "./assets/images/logo-gbg-white.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5cd7538f-3fae-4c93-935e-e25ee854dd07"
      }
    }
     
  }
}
